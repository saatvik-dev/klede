{
  "version": 2,
  "buildCommand": "./build-for-vercel.sh",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}